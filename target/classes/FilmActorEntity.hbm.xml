<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.FilmActorEntity" table="film_actor" schema="public" catalog="test12345">
        <id name="id">
            <column name="id" sql-type="bigint"/>
            <generator class="identity"/>
        </id>
        <property name="actorid">
            <column name="actorid" sql-type="bigint"/>
        </property>
        <property name="filmid">
            <column name="filmid" sql-type="bigint"/>
        </property>
    </class>
</hibernate-mapping>