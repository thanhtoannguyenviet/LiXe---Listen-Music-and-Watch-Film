<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.CategorysongEntity" table="CategorySong" schema="dbo" catalog="ProjectMusicFilm">
        <id name="id">
            <column name="id" sql-type="bigint"/><generator class="identity"/>

        </id>
        <property name="categoryname">
            <column name="categorysongname" sql-type="varchar(255)"/>
        </property>
        <property name="range">
            <column name="range" sql-type="integer"/>
        </property>
    </class>
</hibernate-mapping>