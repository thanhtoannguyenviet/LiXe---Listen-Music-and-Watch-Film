<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.ImageEntity" table="image" schema="public" catalog="test12345">
        <id name="id">
            <column name="id" sql-type="bigint"/>
            <generator class="identity"/>
        </id>
        <property name="path">
            <column name="path" sql-type="varchar(255)"/>
        </property>
        <property name="size">
            <column name="size" sql-type="numeric" precision="-1"/>
        </property>
        <property name="fileextension">
            <column name="fileextension" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="model">
            <column name="model" sql-type="varchar(255)"/>
        </property>
        <property name="entryid">
            <column name="entryid" sql-type="bigint"/>
        </property>
    </class>
</hibernate-mapping>