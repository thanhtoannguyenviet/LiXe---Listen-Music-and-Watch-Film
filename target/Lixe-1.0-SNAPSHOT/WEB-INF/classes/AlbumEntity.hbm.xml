<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.AlbumEntity" table="album" schema="public" catalog="test12345">
        <id name="id">
            <column name="id" sql-type="bigint"/><generator class="identity"/>

        </id>
        <property name="albumname">
            <column name="albumname" sql-type="varchar(255)"/>
        </property>
        <property name="datereleased">
            <column name="datereleased" sql-type="timestamp"/>
        </property>
        <property name="index">
            <column name="index" sql-type="integer"/>
        </property>
        <property name="range">
            <column name="range" sql-type="integer"/>
        </property>
        <property name="singerid">
            <column name="singerid" sql-type="integer"/>
        </property>
    </class>
</hibernate-mapping>