<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.SeriCategoryfilmEntity" table="Seri_Categoryfilm" schema="dbo" catalog="ProjectMusicFilm">
        <id name="id">
            <column name="id" sql-type="bigint"/>
        </id>
        <property name="categoryid">
            <column name="categoryid" sql-type="bigint"/>
        </property>
        <property name="seriid">
            <column name="seriid" sql-type="bigint"/>
        </property>
    </class>
</hibernate-mapping>