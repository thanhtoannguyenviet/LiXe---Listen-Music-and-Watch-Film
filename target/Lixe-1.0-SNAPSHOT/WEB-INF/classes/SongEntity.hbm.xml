<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.SongEntity" table="Song" schema="public" catalog="ProjectMusicFilm">
        <id name="id">
            <column name="id" sql-type="bigint"/>
            <generator class="identity"/>
        </id>
        <property name="songName">
            <column name="songName" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="authorid">
            <column name="authorid" sql-type="bigint" not-null="true"/>
        </property>
        <property name="createdate">
            <column name="createdate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="modifiedUser">
            <column name="modifiedUser" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="modifieddate">
            <column name="modifieddate" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="active">
            <column name="active" sql-type="bit" not-null="true"/>
        </property>
        <property name="uploadsource">
            <column name="uploadsource" sql-type="text" not-null="true"/>
        </property>
        <property name="img">
            <column name="img" sql-type="text" not-null="true"/>
        </property>
        <property name="albumid">
            <column name="albumid" sql-type="bigint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>