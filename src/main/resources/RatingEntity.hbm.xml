<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.RatingEntity" table="Rating" schema="public" catalog="ProjectMusicFilm">
        <id name="id">
            <column name="id" sql-type="bigint"/>
            <generator class="identity"/>
        </id>
        <property name="model">
            <column name="model" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="point">
            <column name="point" sql-type="integer" not-null="true"/>
        </property>
        <property name="votelike">
            <column name="votelike" sql-type="integer" not-null="true"/>
        </property>
        <property name="votedislike">
            <column name="votedislike" sql-type="integer" not-null="true"/>
        </property>
        <property name="entryid">
            <column name="entryid" sql-type="bigint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>