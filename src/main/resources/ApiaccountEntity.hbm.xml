<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.ApiaccountEntity" table="APIAccount" schema="dbo" catalog="ProjectMusicFilm">
        <id name="id">
            <column name="id" sql-type="bigint"/><generator class="identity"/>

        </id>
        <property name="token">
            <column name="token" sql-type="varchar(35)" length="35"/>
        </property>
        <property name="type">
            <column name="type" sql-type="integer"/>
        </property>
    </class>
</hibernate-mapping>