<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.LogEntity" table="log" schema="public" catalog="test12345">
        <id name="id">
            <column name="id" sql-type="bigint"/><generator class="identity"/>

        </id>
        <property name="messageerror">
            <column name="messageerror" sql-type="text" not-null="true"/>
        </property>
        <property name="codeerror">
            <column name="codeerror" sql-type="text" not-null="true"/>
        </property>
        <property name="nameapierror">
            <column name="nameapierror" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>