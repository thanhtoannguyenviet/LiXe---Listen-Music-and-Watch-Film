<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.UploadEntity" table="Upload" schema="public" catalog="ProjectMusicFilm">
        <id name="id">
            <column name="id" sql-type="bigint"/>
            <generator class="identity"/>
        </id>
        <property name="source">
            <column name="source" sql-type="varchar(500)" length="500" not-null="true"/>
        </property>
        <property name="model">
            <column name="model" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="entryid">
            <column name="entryid" sql-type="bigint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>