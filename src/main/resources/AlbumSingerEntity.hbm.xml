<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.AlbumSingerEntity" table="album_singer" schema="public" catalog="test12345">
        <id name="id">
            <column name="id" sql-type="bigint"/><generator class="identity"/>

        </id>
        <property name="albumid">
            <column name="albumid" sql-type="bigint" not-null="true"/>
        </property>
        <property name="singerid">
            <column name="singerid" sql-type="bigint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>