<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Server.model.DB.PlayListEntity" table="play_list" schema="public" catalog="test12345">
        <id name="id">
            <column name="id" sql-type="bigint"/><generator class="identity"/>

        </id>
        <property name="name">
            <column name="name" sql-type="varchar(250)" length="250" not-null="true"/>
        </property>
        <property name="ext">
            <column name="ext" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>